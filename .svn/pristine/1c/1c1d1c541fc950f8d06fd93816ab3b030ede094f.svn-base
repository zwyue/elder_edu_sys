package com.zrtjoa.entity;

import java.util.Date;

/**
 * @Author: yangli
 * @Description: 教室表
 * @Date: Created in 10:12 2018/11/13
 * @Modified by:
 */
public class Classroom {
    //主键id
    private int id;
    //教室名称
    private String jsmc;
    //用途
    private String yt;
    //教室地址
    private String jsdz;
    //备注
    private String bz;
    //是否可用(0：是；1：否)
    private String sfky;
    //创建时间
    private Date cjsj;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getJsmc() {
        return jsmc;
    }

    public void setJsmc(String jsmc) {
        this.jsmc = jsmc;
    }

    public String getYt() {
        return yt;
    }

    public void setYt(String yt) {
        this.yt = yt;
    }

    public String getJsdz() {
        return jsdz;
    }

    public void setJsdz(String jsdz) {
        this.jsdz = jsdz;
    }

    public String getBz() {
        return bz;
    }

    public void setBz(String bz) {
        this.bz = bz;
    }

    public String getSfky() {
        return sfky;
    }

    public void setSfky(String sfky) {
        this.sfky = sfky;
    }

    public Date getCjsj() {
        return cjsj;
    }

    public void setCjsj(Date cjsj) {
        this.cjsj = cjsj;
    }
}
