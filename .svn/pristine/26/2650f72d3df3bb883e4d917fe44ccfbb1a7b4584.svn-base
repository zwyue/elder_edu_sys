  <sql id="Base_Column_List">
    id, stunumber, sfzh, stuname, classid, classname, sex, nation,
    phone, emergency, sfzaddress, address, carnumber, photo, status,
    DATE_FORMAT(crttime,'%Y-%m-%d') crttime,
    DATE_FORMAT(updatetime,'%Y-%m-%d %H:%i:%S') updatetime,
    DATE_FORMAT(entertime,'%Y-%m-%d %H:%i:%S') entertime,
    DATE_FORMAT(graduatetime,'%Y-%m-%d %H:%i:%S') graduatetime
  </sql>

  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from student
    where id = #{id}
  </select>

  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from student
    where id = #{id,jdbcType=INTEGER}
  </delete>

  <!-- 学生录入 返回主键-->
  <insert id="insert" parameterType="com.zrtjoa.entity.Student"
          useGeneratedKeys="true"
          keyColumn="id"
          keyProperty="id">
    insert into student (
        stunumber, sfzh, stuname, sex, age, classid, classname,nation,phone,emergency,
        sfzaddress, address,photo, status, crttime, updatetime,entertime, graduatetime
        <if test="carnumber != null and carnumber != ''">,carnumber</if>
    ) values (
        #{stunumber}, #{sfzh}, #{stuname},#{sex},#{age}, #{classid},#{classname},#{nation},#{phone},
        #{emergency}, #{sfzaddress},#{address},#{photo},0,now(), now(), #{entertime},#{graduatetime}
        <if test="carnumber != null and carnumber != ''">,#{carnumber}</if>
    )
    <selectKey resultType="int" keyProperty="id" order="AFTER">
      SELECT LAST_INSERT_ID()
    </selectKey>
  </insert>

