  <select id="getSList" parameterType="int" resultType="com.zrtjoa.entity.Student">
    select
    <include refid="Base_Column_List"/>
    from student where classid LIKE concat('%',#{classid},'%');
  </select>
  <select id="getStudentList" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from student
    where  id  not in ( select sid from studymeet);
  </select>
  <select id="getNameList" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from student
    where  id  not in ( select sid from studymeet)
    and stuname LIKE concat('%',#{name},'%');
  </select>
  <select id="queryStudentByIdCard" parameterType="string" resultType="com.zrtjoa.entity.Student">
    select
    <include refid="Base_Column_List"/>
    from student where sfzh = #{sfzh}
  </select>

  <select id="queryAllStudents" resultType="com.zrtjoa.entity.Student" parameterType="map">
    SELECT
      t.*
    FROM
      student t
      LEFT JOIN studententer e ON t.id = e.stuid
    WHERE
      e.term_id = #{termId}
    <if test="name!=null and name != ''">
      and t.stuname LIKE concat('%',#{name},'%')
    </if>
    <if test="idCard!=null and idCard != ''">
      and t.sfzh LIKE concat('%',#{idCard},'%')
    </if>
  </select>

  <select id="queryStuNo" resultType="string">
    select stunumber from student;
  </select>

  <update id="updateStudentNumbers" parameterType="java.util.Map">
    update student set stunumber = #{studentNumbers} ,classid = #{classId},
                       classname = #{className} where id = #{id}
  </update>
