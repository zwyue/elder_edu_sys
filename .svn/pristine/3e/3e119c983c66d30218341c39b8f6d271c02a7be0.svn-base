package com.zrtjoa.entity;

import java.util.Date;

/**
 * @Author: yangli
 * @Description: 班级表
 * @Date: Created in 10:07 2018/11/13
 * @Modified by:
 */
public class Grades {
    //主键id
    private int id;
    //班级序号
    private int bjxh;
    //班级名称
    private String bjmc;
    //班级人数
    private int bjrs;
    //专业id
    private int zyid;
    //专业名称
    private String zymc;
    //教师ID
    private int jsid;
    //教师名称
    private String jsmc;
    //创建时间
    private Date cjsj;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public int getBjxh() {
        return bjxh;
    }

    public void setBjxh(int bjxh) {
        this.bjxh = bjxh;
    }

    public String getBjmc() {
        return bjmc;
    }

    public void setBjmc(String bjmc) {
        this.bjmc = bjmc;
    }

    public int getBjrs() {
        return bjrs;
    }

    public void setBjrs(int bjrs) {
        this.bjrs = bjrs;
    }

    public int getZyid() {
        return zyid;
    }

    public void setZyid(int zyid) {
        this.zyid = zyid;
    }

    public String getZymc() {
        return zymc;
    }

    public void setZymc(String zymc) {
        this.zymc = zymc;
    }

    public int getJsid() {
        return jsid;
    }

    public void setJsid(int jsid) {
        this.jsid = jsid;
    }

    public String getJsmc() {
        return jsmc;
    }

    public void setJsmc(String jsmc) {
        this.jsmc = jsmc;
    }

    public Date getCjsj() {
        return cjsj;
    }

    public void setCjsj(Date cjsj) {
        this.cjsj = cjsj;
    }
}
