		<script>
		$(function(){
		   $('navTree li .nav_li_a').click(function(event){
		       var dd = $(this).next('dl');
		       var display = dd.css('display');
		       if (display == 'none'){
		           $("#navTree dl").hide().eq($('navTree li .nav_li_a').index(this)).show(); 
		       }else{
		           dd.css('display','none');
		       }
		   });
		})
		</script>
