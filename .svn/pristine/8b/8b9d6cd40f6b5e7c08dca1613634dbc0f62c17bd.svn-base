package com.zrtjoa.entity;

import java.util.Date;

/**
 * @Author: yangli
 * @Description: 权限表
 * @Date: Created in 9:23 2018/11/13
 * @Modified by:
 */
public class Authority {
    //主键id
    private int id;
    //权限名称
    private String qxmc;
    //权限规则
    private String qxgz;
    //所属分类
    private String ssfl;
    //创建时间
    private Date cjsj;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getQxmc() {
        return qxmc;
    }

    public void setQxmc(String qxmc) {
        this.qxmc = qxmc;
    }

    public String getQxgz() {
        return qxgz;
    }

    public void setQxgz(String qxgz) {
        this.qxgz = qxgz;
    }

    public String getSsfl() {
        return ssfl;
    }

    public void setSsfl(String ssfl) {
        this.ssfl = ssfl;
    }

    public Date getCjsj() {
        return cjsj;
    }

    public void setCjsj(Date cjsj) {
        this.cjsj = cjsj;
    }
}
