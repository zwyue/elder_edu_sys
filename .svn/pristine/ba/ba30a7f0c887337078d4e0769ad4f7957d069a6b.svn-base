package com.zrtjoa.entity;

import java.util.Date;

/**
 * @Author: yangli
 * @Description: 专业表
 * @Date: Created in 10:21 2018/11/13
 * @Modified by:
 */
public class Profession {
    //主键id
    private int id;
    //专业序号
    private int zyxh;
    //专业名称
    private String zymc;
    //创建时间
    private Date cjsj;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public int getZyxh() {
        return zyxh;
    }

    public void setZyxh(int zyxh) {
        this.zyxh = zyxh;
    }

    public String getZymc() {
        return zymc;
    }

    public void setZymc(String zymc) {
        this.zymc = zymc;
    }

    public Date getCjsj() {
        return cjsj;
    }

    public void setCjsj(Date cjsj) {
        this.cjsj = cjsj;
    }
}
