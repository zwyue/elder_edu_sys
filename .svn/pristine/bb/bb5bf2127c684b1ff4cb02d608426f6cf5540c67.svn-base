     //验证身份证
     var idNo = $('#idCard').val();
     var regIdNo = /(^\d{15}$)|(^\d{18}$)|(^\d{17}(\d|X|x)$)/;
     if(idNo==''){
        layer.alert('请输入身份证号');
        return false;
     }else if(!regIdNo.test(idNo)){
        layer.alert('身份证号填写有误');
        return false;
     };
