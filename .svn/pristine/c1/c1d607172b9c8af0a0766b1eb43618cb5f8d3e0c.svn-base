
/*导出按钮*/
.btn_chaXun{
    position:absolute; right:2%; width: 100px;height:40px;background: #3d9def;color: #FFFFFF;
    text-align: center;line-height: 40px;font-size: 1.1rem;border-radius:20px;letter-spacing: 0.2rem;
    box-shadow:0px 5px 10px #b0cdfc;
}
.btn_chaXun:hover{
    color:#FFFFFF;
}
  /*内容主区域*/
  .body_contain{
      background-color: #f3f3f3;
      position: fixed;
      bottom: 0!important;
  }

  .body_header{
      padding:0.7rem 0 0.7rem 0 ;
      font-size: 18px;
      padding-left: 1%;
  }
  .body_header span a{
      font-size: 18px;
  }
  .body_header .txt_xsl2:hover{
      color:#000000;
  }
  .body_header .txt_xsl3{
      color:#666666;
  }
  .body_header button{
      float: right;
      background-color: #3d9def;
      background-color: #ff9227;
      background: #3d9def;
      color: #ffffff;
      border: none;
      padding:0.2rem 0.8rem 0.2rem 0.8rem;
      border-radius: 0.3rem;
      margin-right:1%;
      cursor:pointer;
  }
  /*下拉框区域*/
  .select{
      width: 98%;
      height: 6rem;
      background-color: #FFFFFF;
      margin-left: 1%;
      border-radius: 0.3rem;
  }
  .select .select_auto{
      padding:1.8rem 0 0.5rem 1.5rem;
      border-radius: 0.5rem;
  }
  .select span{
      border-radius: 0.5rem;
      padding-top: 0.5rem;
  }
  /*下拉框区域end*/

  /*折线图区域*/
  .body_zxt{
      width: 98%;
      height: 100%;
      background-color: #FFFFFF;
      margin-left: 1%;
      border-radius: 0.3rem;
      margin-top: 1rem;
  }
  /*折线图区域end*/
